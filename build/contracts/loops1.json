  "sha1": "d8a73f569b87457e6c0f7043c78e0f883a354dfd",
  "source": "pragma solidity ^0.5.0;\n\ncontract loops1 {\n    \n    uint256 public counter;\n    \n    constructor() public {\n        counter = 0;\n    }\n    \n    function loop1() public {\n        for (uint256 i = 0; i < 10; i++) {\n            counter++;\n        }\n    }\n    \n    function loop2() public {\n        uint256 j = 0;\n        while (j < 10) {\n            counter++;\n            j++;\n        }\n    }\n}",
